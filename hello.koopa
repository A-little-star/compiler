fun @half(@x: i32): i32 {
%entry:
  %x = alloc i32
  store @x, %x
  %0 = load %x
  %1 = div %0, 2
  ret %1
}
fun @f(@y: i32):  {
%entry:
  @t_1 = alloc i32
  %y = alloc i32
  store @y, %y
  %2 = load %y
  %3 = call @half(%2)
  store %3, @t_1
  ret
}
fun @main(): i32 {
%entry:
  @x_1 = alloc i32
  store 1, @x_1
  call @f()
  %4 = load @x_1
  %5 = call @half(%4)
  ret %5
}
