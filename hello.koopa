fun @main(): i32 {
%entry:
  @x = alloc i32
  store 100, @x
  @y = alloc i32
  %0 = load @x
  %1 = mul %0, 3
  %2 = load @x
  %3 = mod %2, 9
  %4 = add %1, %3
  store %4, @y
  @z = alloc i32
  %5 = load @x
  %6 = load @y
  %7 = add %5, %6
  %8 = load @y
  %9 = mul %7, %8
  store %9, @z
  %10 = load @z
  %11 = add %10, 1
  ret %11
}
